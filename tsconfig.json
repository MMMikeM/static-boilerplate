{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "types": ["bun-types"],
    "jsxImportSource": "react",
    "paths": {
      "~/*": ["./src/*"],
      "@primitives/*": ["src/components/primitives/*"],
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"]
    }
  },
  "extends": "astro/tsconfigs/strictest"
}
